{
  "name": "Clean Quora",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "This extension cleans quora feed by removing ads, links and suggestions.",
  "homepage_url": "http://www.niyando.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": [
        "https://www.quora.com/*"
      ],
      "js": [
        "js/jquery/jquery.min.js","src/inject/inject.js"
      ]
    }
  ]
}